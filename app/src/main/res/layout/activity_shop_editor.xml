            app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.6"
            app:layout_constraintVertical_bias="0.173"/>
    <EditText
            android:layout_width="220dp"
            android:layout_height="37dp"
            android:inputType="text"
            android:gravity="start|top"
            android:ems="10"
            android:id="@+id/shop_description_field" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.91"
            app:layout_constraintVertical_bias="0.217"/>
    <EditText
